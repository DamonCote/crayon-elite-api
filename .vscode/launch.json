{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "attach",
            "name": "Nodemon Debug",
            "port": 9229,
            "restart": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "CrayonEliteApi",
            "skipFiles": ["node_modules/**"],
            "env": {
                "NODE_ENV": "default"
            },
            "envFile": "${workspaceFolder}/.env",
            "program": "${workspaceFolder}/src/start.js",
            "runtimeExecutable": "${env:HOME}/.nvm/versions/node/v18.20.0/bin/node"
        }
    ]
}
